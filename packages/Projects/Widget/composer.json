{
    "name": "projects/widget",
    "description": "Package for learning",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Olga Zhilkova",
            "email": "mindfulbayhorse@gmail.com"
        }
    ],
    "keywords": ["Laravel", "Widget"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Projects\\Widget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Projects\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Projects\\Widget\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "Projects\\Widget\\Facades\\Widget"
            }
        }
    }
}
